{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vitejs/plugin-react"
      }
    ],
    "routes": [
      {
        "src": "/blogs",
        "dest": "/"
      },
      {
        "src": "/about",
        "dest": "/"
      },
      {
        "src": "/projects",
        "dest": "/"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }